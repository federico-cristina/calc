use std::math;

type Square = object {
    side : real;

    pub Square(side : real) {
        this.side = side;
    }

    pub fn Area(&this : Square) -> real
        = sqre(this.side);

    pub fn Perimeter(&this : Square) -> real
        = 4 * this.side;
};

type Cube = object : Square {
    pub Cube(side : real) {
        base(side);
    }

    pub fn Area(&this : Cube) -> real
        = 6 * base.Area();

    pub fn Perimeter(&this : Cube) -> real
        = 12 * this.side();
};
